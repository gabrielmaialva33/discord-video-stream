    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/libsodium-wrappers": "^0.7.13",
    "@types/node": "^20.12.5",
    "@types/ws": "^8.5.10",
    "eslint": "^9.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.4"
  },
  "peerDependencies": {
    "discord.js-selfbot-v13": "3.x"
